        <activity
            android:name=".NewANMActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MotherActivity"
            android:screenOrientation="portrait"
